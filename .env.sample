NEXT_PUBLIC_ADMIN_ADDRESS=
NEXT_PUBLIC_CONTRACT_ADDRESS=
NEAR_NETWORK=testnet  # Use 'testnet' for development, 'mainnet' for production
NEAR_ACCOUNT_ID= # Your NEAR account ID
NEAR_PRIVATE_KEY=  # Your account's private key
NEAR_RPC_URL=https://rpc.testnet.near.org

# LLM Configuration (Required)
LLM_PROVIDER=hyperbolic  # Currently supported: hyperbolic
LLM_API_KEY=hybolic-key-here
LLM_MODEL=meta-llama/llama-3.3-70B-Instruct  # Default LLM model
LLM_TEMPERATURE=0.7  # Lower for more deterministic responses
LLM_MAX_TOKENS=2000  # Maximum response length
LLM_API_URL=https://api.hyperbolic.xyz/v1

# Linea RPC URLs
LINEA_RPC_URL=https://rpc.linea.build
LINEA_TESTNET_RPC_URL=https://rpc.goerli.linea.build

# Linea API Keys for verification
LINEA_API_KEY=your_linea_api_key
LINEA_TESTNET_API_KEY=your_linea_testnet_api_key

# Deployment
PRIVATE_KEY=your_private_key

# Contract Addresses (will be filled after deployment)
CROAK_TOKEN_ADDRESS=
FROG_NFT_ADDRESS=
CROAK_CLASH_ADDRESS=
